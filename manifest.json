{
	"manifest_version":2,
	"name":"NoAltShift",
	"description":"This extension shows a Google Image search result for the current page",
	 "version": "0.001",
	 
	 "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "NoAltShift"
  },
 "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]


}